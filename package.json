{
  "name": "wikibattle",
  "description": "1v1 races through Wikipedia article links.",
  "version": "1.1.0",
  "license": "MIT",
  "bugs": "https://github.com/goto-bus-stop/WikiBattle/issues",
  "author": "goto-bus-stop <rene@kooi.me>",
  "bin": "bin/wikibattle",
  "dependencies": {
    "JSONStream": "^1.2.1",
    "after": "^0.8.2",
    "bus": "^0.1.0",
    "cheerio": "^0.22.0",
    "closest": "0.0.1",
    "component-classes": "^1.2.4",
    "component-delegate": "^0.2.3",
    "component-emitter": "^1.2.1",
    "compression": "^1.6.1",
    "crel": "^3.0.0",
    "crypto-random-string": "^1.0.0",
    "debug": "^2.6.0",
    "dom-event": "^1.0.0",
    "each-async": "^1.1.1",
    "empty-element": "^1.0.0",
    "express": "^4.13.3",
    "inflight": "^1.0.4",
    "map-async": "^0.1.1",
    "ms": "^1.0.0",
    "newless": "^0.3.0",
    "node-schedule": "^1.2.0",
    "random-item": "^1.0.0",
    "request": "^2.64.0",
    "serve-static": "^1.10.2",
    "throttleit": "^1.0.0",
    "ws": "^2.0.0",
    "ws-events": "^1.0.0",
    "xhr": "^2.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-env": "^1.4.0",
    "babelify": "^7.2.0",
    "babili": "0.0.12",
    "browserify": "^14.0.0",
    "bundle-collapser": "^1.2.1",
    "cssnano-cli": "^1.0.5",
    "less": "^2.5.3",
    "rollupify": "^0.3.9",
    "standard": "^10.0.0"
  },
  "repository": "goto-bus-stop/WikiBattle",
  "scripts": {
    "start": "bin/wikibattle",
    "lint": "standard src/**/*.js",
    "test": "npm run lint",
    "less": "lessc ./public/stylesheets/style.less > ./public/stylesheets/style.css",
    "update": "node update.js",
    "compile:server": "babel --out-dir lib src",
    "compile:client": "browserify --debug -t [ rollupify --config rollup.config.js ] -t babelify -e public/javascripts/index.js -o public/app.js",
    "compile": "npm run compile:server && npm run compile:client",
    "minify-css": "cssnano public/stylesheets/style.css public/stylesheets/style.css",
    "minify-js": "bundle-collapser public/app.js | babili -o public/app.js",
    "dev": "npm run compile && npm run less",
    "prod": "npm run less && npm run minify-css && npm run compile && npm run minify-js",
    "build": "npm run prod && npm run update",
    "prepublish": "npm run build"
  }
}
